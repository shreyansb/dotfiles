[user]
	name = Shreyans Bhansali
	email = shreyans@venmo.com
[core]
	excludesfile = /Users/shreyans/.gitignore
[rerere]
	enabled = 1
[branch "master"]
	remote = origin
	merge = refs/heads/master
[alias]
	br = branch
	ci = commit -a
	co = checkout
	df = diff
	dwc = diff -w -M --color-words
	lg = log
	ll = log -p
	rb = rebase
	fix = rebase -i origin/master
	rbc = rebase --continue
	re = remote
	st = status
	tree = log --graph --decorate --pretty=oneline --abbrev-commit --all
	up = pull --rebase
	master-rebase = "!git fetch origin master && git rebase master"
[color]
	ui=auto
[dude]
	interval = 30
	icon = /Users/shreyans/.git-dude/github_32.png
